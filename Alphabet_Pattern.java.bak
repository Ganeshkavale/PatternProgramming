class Alphabet_Pattern
{
	public static void main(String[] args) 
	{
		char ch = 'z';

		for (int i = 1; i <=4 ; i++ )
		{
			for (int j = 1; j <=4 ; j++ )
			{
				if (i %2 ==0)
				{
					System.out.print(ch + " ");
					ch--;
				}
				else
				{
					System.out.print(ch + " ");
					ch--;
				}
			

			}
			System.out.println();

		}

		
	}
}
